coroutine.wrap(function()
    repeat
        wait()
    until game:IsLoaded()
    print("Game is fully loaded!")
end)()


coroutine.wrap(function()
    repeat
        wait()
    until game.Players.LocalPlayer.Character
    print("Local player character is loaded!")
end)()

